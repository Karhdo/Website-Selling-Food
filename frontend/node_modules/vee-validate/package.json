{
  "name": "vee-validate",
  "version": "3.4.14",
  "description": "Template Based Validation Framework for Vue.js",
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "module": "dist/vee-validate.esm.js",
  "unpkg": "dist/vee-validate.js",
  "main": "dist/vee-validate.js",
  "types": "dist/types/index.d.ts",
  "homepage": "https://vee-validate.logaretm.com/v3",
  "repository": "https://github.com/logaretm/vee-validate",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "lint": "eslint **/*.{js,jsx,ts,tsx} --fix",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build:locale": "node scripts/locale.js",
    "build:main": "node scripts/main.js",
    "ts:defs": "node scripts/definitions.js",
    "build": "npm run build:main && npm run ts:defs && npm run build:locale",
    "cover": "jest --coverage",
    "cover:ci": "jest --coverage --ci --runInBand --detectOpenHandles",
    "prepublishOnly": "npm run build && npm run ts:defs"
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "files": [
    "dist/*.js",
    "dist/locale/*.json",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^9.1.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@vue/server-test-utils": "^1.1.0",
    "@vue/test-utils": "1.1.0",
    "@vuepress/plugin-back-to-top": "^1.5.4",
    "@vuepress/plugin-google-analytics": "^1.5.4",
    "@vuepress/plugin-pwa": "^1.5.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "chalk": "^4.1.0",
    "codesandbox": "^2.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fast-deep-equal": "^3.1.3",
    "filesize": "^6.1.0",
    "flush-promises": "^1.0.2",
    "fs-extra": "^9.0.1",
    "gzip-size": "^5.1.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "markdown-it-custom-block": "^0.1.1",
    "mkdirp": "^1.0.4",
    "prettier": "^1.19.1",
    "raf-stub": "^3.0.0",
    "rollup": "^2.50.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "terser": "^5.7.0",
    "ts-jest": "^26.4.0",
    "tslib": "^2.0.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.0.3",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.5.4"
  },
  "eslintIgnore": [
    "locale",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ]
  },
  "peerDependencies": {
    "vue": "^2.5.18"
  }
}
